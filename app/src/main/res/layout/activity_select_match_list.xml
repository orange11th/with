<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context=".selectMatch.SelectMatchList">
    <androidx.appcompat.widget.Toolbar
        app:titleTextColor="#5D8735"
        android:background="#FFFFFF"
        android:id="@+id/toolbar_list"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"
        tools:ignore="MissingConstraints"
        />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="27dp"
        android:layout_marginLeft="27dp"
        android:layout_marginTop="28dp"
        android:text="모든 지역"
        android:textColor="#000000"
        android:textSize="24dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_list"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="132dp"
        android:layout_marginLeft="132dp"
        android:layout_marginTop="44dp"
        android:text="지역변경"
        android:textColor="#8d8d8d"
        android:textSize="12dp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/toolbar_list"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/tag0"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="130dp"
        android:text="전체보기"
        android:backgroundTint="#96C866"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tag3"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="33dp"
        android:text="#가사돌봄"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/tag0" />

    <Button
        android:id="@+id/tag1"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="130dp"
        android:text="#병원동행"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/tag0"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/tag4"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="33dp"
        android:text="#말동무"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/tag3"
        app:layout_constraintTop_toTopOf="@id/tag1" />

    <Button
        android:id="@+id/tag5"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="33dp"
        android:text="#취미/여가"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/tag4"
        app:layout_constraintTop_toTopOf="@id/tag1" />

    <Button
        android:id="@+id/tag2"
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="130dp"
        android:text="#이동보조"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        app:layout_constraintLeft_toRightOf="@id/tag1"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:layout_width="85dp"
        android:layout_height="25dp"
        android:layout_marginStart="10dp"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="130dp"
        android:background="@drawable/hashtagbackground"
        android:gravity="center"
        android:text="#기타"
        app:layout_constraintLeft_toRightOf="@id/tag2"
        app:layout_constraintTop_toTopOf="parent" />

    <ListView
        android:id="@+id/listtest"
        android:layout_width="371dp"
        android:layout_height="match_parent"

        android:layout_marginStart="20dp"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="200dp"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:textColor="#000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

</androidx.constraintlayout.widget.ConstraintLayout>